{
  "name": "@opentrons/ot-app",
  "version": "3.0.0-alpha",
  "description": "Opentrons desktop application",
  "main": "shell/main.js",
  "scripts": {
    "test": "make test",
    "start": "make dev",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenTrons/opentrons.git"
  },
  "author": {
    "name": "Opentrons Labworks",
    "email": "engineering@opentrons.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/OpenTrons/opentrons/issues"
  },
  "homepage": "https://github.com/OpenTrons/opentrons",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|png|gif|svg|woff|woff2)$": "<rootDir>/ui/__mocks__/file.js",
      "\\.(css)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/__mocks__/**",
      "!**/dist/**",
      "!**/webpack*",
      "!**/webpack/**",
      "!**/coverage/**",
      "!**/test/**"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist/"
    ],
    "env": [
      "node",
      "browser",
      "jest"
    ]
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-css-modules"
    ],
    "ignoreFiles": [
      "**/dist/**",
      "**/coverage/**"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-register": "^6.24.1",
    "bonjour": "^3.5.0",
    "classnames": "^2.2.5",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "electron": "1.6.11",
    "electron-builder": "~19.27.3",
    "electron-builder-squirrel-windows": "~19.27.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^1.1.5",
    "handlebars-loader": "^1.6.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "lodash": "^4.17.4",
    "portfinder": "^1.0.13",
    "prop-types": "^15.5.10",
    "randomstring": "^1.1.5",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "script-ext-html-webpack-plugin": "^1.8.5",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "style-loader": "^0.18.1",
    "stylelint": "~8.0.0",
    "stylelint-config-css-modules": "^1.1.0",
    "stylelint-config-standard": "^17.0.0",
    "url-loader": "^0.6.2",
    "webpack": "~2.6.1",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dev-server": "~2.4.5",
    "winston": "^2.2.0",
    "worker-loader": "^1.1.0",
    "ws": "^3.1.0"
  },
  "dependencies": {
    "electron-debug": "^1.1.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-json-storage": "^2.0.0",
    "electron-log": "^2.2.9",
    "electron-settings": "^2.2.4",
    "electron-squirrel-startup": "^1.0.0",
    "moment": "^2.19.1",
    "shelljs": "^0.7.8",
    "zip-folder": "^1.0.0"
  }
}
